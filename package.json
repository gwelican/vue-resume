{
  "name": "vue-resume",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode=development",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "pdf": "node export.js",
    "ssr:build": "vue-cli-service ssr:build",
    "ssr:fix": "vue-cli-service ssr:fix",
    "ssr:fix-vuex": "vue-cli-service ssr:fix-vuex",
    "ssr:serve": "vue-cli-service ssr:serve",
    "ssr:start": "vue-cli-service ssr:start",
    "ssr:static": "vue-cli-service ssr:static",
    "export": "concurrently \"npm run serve\" \"npm run pdf\" --success first --kill-others --raw"
  },
  "dependencies": {
    "@uvue/core": "^0.1.0",
    "@uvue/server": "^0.1.7",
    "compression": "^1.7.3",
    "connect": "^3.6.6",
    "cookie-parser": "^1.4.3",
    "core-js": "^3.6.4",
    "serve-static": "^1.13.2",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "vue-server-renderer": "^2.6.11",
    "vuetify": "^2.2.11"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "puppeteer": "^3.0.4",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@uvue/vue-cli-plugin-ssr": "~0.1.6",
    "@vue/cli-plugin-babel": "~4.5.13",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.15.1",
    "sass": "^1.34.0",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-vuetify": "~2.4.0",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.7.2"
  }
}
